<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <root>
      <AnchorPane prefHeight="200" prefWidth="200">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label fx:id="maxEvents" alignment="CENTER" prefHeight="66.0" prefWidth="168.0" text="Enter a max number of tasks" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <TextField fx:id="numMaxEvents" maxWidth="-Infinity" prefHeight="50.0" prefWidth="150.0" />
                  <Button fx:id="buttonMaxEvents" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Set" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </root>
</Scene>
